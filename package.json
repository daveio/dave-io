  "devDependencies": {
    "@anthropic-ai/claude-code": "^1.0.81",
    "@catppuccin/daisyui": "^2.1.1",
    "@catppuccin/tailwindcss": "^1.0.0",
    "@iconify-json/heroicons": "^1.2.2",
    "@iconify-json/mdi": "^1.2.3",
    "@iconify-json/oui": "^1.2.6",
    "@iconify-json/simple-icons": "^1.2.47",
    "@sentry/toolbar": "^1.0.0-beta.11",
    "@tailwindcss/cli": "^4.1.12",
    "@trunkio/launcher": "^1.3.4",
    "@types/lodash-es": "^4.17.12",
    "nitro-cloudflare-dev": "^0.2.2",
    "nitropack": "^2.12.4",
    "npm-run-all2": "^8.0.4",
    "nuxi": "^3.28.0",
    "prettier": "^3.6.2",
    "supabase": "^2.34.3",
    "untun": "^0.1.3",
    "vite-plugin-eslint2": "^5.0.4",
    "wrangler": "^4.30.0"
  },
  "name": "rebuild-dave-io",
  "overrides": {
    "@vercel/nft": "^0.30.0"
  },
  "private": true,
  "scripts": {
    "build": "bun run nuxt build",
    "deploy": "bun run-s build deploy:wrangler",
    "deploy:wrangler": "bun run wrangler deploy",
    "deploy:nonprod": "bun run-s build deploy:nonprod:wranglerpp",
    "deploy:nonprod:wrangler": "bun run wrangler versions upload",
    "dev": "bun run nuxt dev",
    "format": "bun run run-s format:prettier format:trunk",
    "format:prettier": "bun run prettier --write .",
    "format:trunk": "bun run trunk fmt -a",
    "generate": "bun run nuxt generate",
    "lint": "bun run run-s lint:eslint lint:trunk lint:types",
    "lint:eslint": "bun run eslint .",
    "lint:eslint:fix": "bun run eslint . --fix",
    "lint:fix": "bun run run-s lint:eslint:fix lint:trunk:fix",
    "lint:trunk": "bun run trunk check -a",
    "lint:trunk:fix": "bun run trunk check -a --fix",
    "lint:types": "bun run tsc --noEmit",
    "postinstall": "bun run nuxt prepare",
    "preview": "bun run-s build preview:wrangler",
    "preview:wrangler": "bun run wrangler dev",
    "types": "bun run-s types:wrangler types:supabase",
    "types:wrangler": "bun run wrangler types",
    "types:supabase": "bun run supabase gen types --db-url $NUXT_SUPABASE_DB_URL > types/database.types.ts",
    "types:supabase:local": "bun run supabase gen types --local > types/database.types.ts"
  },
  "trustedDependencies": [
    "@parcel/watcher",
    "@sentry/cli",
    "@tailwindcss/oxide",
    "esbuild",
    "sharp",
    "supabase",
    "unrs-resolver",
    "workerd"
  ],
  "type": "module"
}
