{
  "name": "rebuild-dave-io",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "bun run nuxt build",
    "dev": "bun run nuxt dev",
    "format": "bun run run-s format:prettier format:trunk",
    "format:prettier": "bun run prettier --write .",
    "format:trunk": "bun run trunk fmt -a",
    "generate": "bun run nuxt generate",
    "lint": "bun run run-s lint:eslint lint:trunk lint:types",
    "lint:fix": "bun run run-s lint:eslint:fix lint:trunk:fix",
    "lint:eslint": "bun run eslint .",
    "lint:eslint:fix": "bun run eslint . --fix",
    "lint:trunk": "bun run trunk check -a",
    "lint:trunk:fix": "bun run trunk check -a --fix",
    "lint:types": "bun run tsc --noEmit",
    "preview": "bun run nuxt preview",
    "postinstall": "bun run-s postinstall:nuxt postinstall:supabase",
    "postinstall:nuxt": "bun run nuxt prepare",
    "postinstall:supabase": "supabase gen types --db-url $SUPABASE_DB_URL > supabase.d.ts"
  },
  "dependencies": {
    "@formkit/auto-animate": "^0.8.2",
    "@nuxt/eslint": "1.8.0",
    "@nuxt/fonts": "0.11.4",
    "@nuxt/icon": "2.0.0",
    "@nuxt/image": "1.11.0",
    "@nuxt/scripts": "0.11.10",
    "@nuxt/test-utils": "3.19.2",
    "@nuxtjs/tailwindcss": "^7.0.0-beta.0",
    "@pinia/nuxt": "^0.11.2",
    "@sentry/nuxt": "^10.5.0",
    "@unhead/vue": "^2.0.14",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "eslint": "^9.33.0",
    "magic-regexp": "^0.10.0",
    "nuxt": "^4.0.3",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "trustedDependencies": [
    "@parcel/watcher",
    "@sentry/cli",
    "@tailwindcss/oxide",
    "@vercel/speed-insights",
    "esbuild",
    "sharp",
    "unrs-resolver"
  ],
  "devDependencies": {
    "@anthropic-ai/claude-code": "^1.0.77",
    "@sentry/toolbar": "^1.0.0-beta.11",
    "@trunkio/launcher": "^1.3.4",
    "npm-run-all2": "^8.0.4",
    "nuxi": "^3.28.0",
    "untun": "^0.1.3",
    "vite-plugin-eslint2": "^5.0.4"
  },
  "overrides": {
    "@vercel/nft": "^0.30.0"
  }
}
