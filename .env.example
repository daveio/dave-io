# Development settings
# API_DEV_DISABLE_RATE_LIMITS="1"
# PI_DEV_USE_DANGEROUS_GLOBAL_KEY="1"

# Cloudflare configuration (required for analytics)
# Method 1: API Token (RECOMMENDED - scoped permissions)
CLOUDFLARE_API_TOKEN="xxx"
CLOUDFLARE_ACCOUNT_ID="xxx"

# Method 2: Global API Key (DANGEROUS - full account access)
# Only used when API_DEV_USE_DANGEROUS_GLOBAL_KEY=1 or "true"
# CLOUDFLARE_API_KEY="xxx"
# CLOUDFLARE_EMAIL="xxx@xxx.xxx"

# Nuxt configuration
NUXT_PUBLIC_API_BASE_URL="/xxx"

# JWT authentication
API_JWT_SECRET="xxx"

# Anthropic Claude (deprecated - kept for reference)
ANTHROPIC_API_KEY="xxx"

# OpenRouter
OPENROUTER_API_KEY="xxx"

# AI Gateway
AI_GATEWAY_TOKEN="xxx"
